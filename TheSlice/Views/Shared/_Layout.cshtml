    <link href="/Themes/DefaultClean/Content/css/styles.css" rel="stylesheet" type="text/css" />
    <link href="/Plugins/Widgets.NivoSlider/Content/nivoslider/nivo-slider.css" rel="stylesheet" type="text/css" />
